    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def sigmoid_derivative(x):\n",
    "    s=1.0/(1+1.0/np.exp(x))\n",
    "    ds=s*(1-s)\n",
    "    return ds"
   ]
  },
  {
